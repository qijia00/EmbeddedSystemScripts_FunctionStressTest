			<when test="equal($target, 'Low')">
				<action code="=($targetno, 0.75)"/>
			</when>
			<when test="equal($target, 'Med')">
				<action code="=($targetno, 0.5)"/>
			</when>
			<when test="equal($target, 'High')">
				<action code="=($targetno, 0.35)"/>
			</when>
		</choose>

		<action code="=($same, PersistB($mode, $target, $targetno))"/>
	</function>

