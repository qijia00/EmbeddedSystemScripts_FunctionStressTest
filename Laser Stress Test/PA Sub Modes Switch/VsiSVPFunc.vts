		<param name="$check"/>

		<action code="=($flag, 'FAIL stay')"/>
		<if test="equal($target, $check)">
			<action code="=($flag, 'PASS break')"/>
			<action code="=($logmsg, 'PASS! Current Baseline is ')"/>
			<action code="+=($logmsg, $check)"/>
