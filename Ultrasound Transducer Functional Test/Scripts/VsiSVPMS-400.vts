	<action code="ScanRestart3('BeamAngle', 'Power-Mode', -15, 'in range')"/>
	<action code="ScanRestart3('BeamAngle', 'Power-Mode', 0, 'in range')"/>
	<action code="ScanRestart3('BeamAngle', 'Power-Mode', 15, 'in range')"/>
	<action code="Frequency('Power-Mode', 30000000, 30000000)"/>
	<action code="ScanRestart3('BeamAngle', 'Power-Mode', -15, 'out of range')"/>
	<action code="ScanRestart3('BeamAngle', 'Power-Mode', 0, 'in range')"/>
	<action code="ScanRestart3('BeamAngle', 'Power-Mode', 15, 'out of range')"/>

	<action code="=($logmsg, ' ')"/>
	<log>script:$logmsg</log>

	<!-- Nonlinear Contrast Mode -->
	<action code="=($logmsg, 'Nonlinear Contrast Mode')"/>
	<log level="warning">script:$logmsg</log>
	
	<action code="=($logmsg, ' ')"/>
	<log>script:$logmsg</log>

	<!-- TST-25-0100 -->
	<action code="=($logmsg, 'TST-25-0100:')"/>
	<log level="warning">script:$logmsg</log>

	<cmd code="Reset"/>		<!-- simulate unplug transducer or reopen the app -->
	<action code="PauseScan()"/>
	<!-- action code="InitializeTransducerSim('MS-400', 'General Imaging')"/ -->	<!-- with simulator -->
	<action code="InitializeTransducer('MS-400', 'General Imaging')"/>	<!-- on system -->
	<sleep time="10000"/>
	<action code="Contrast('NLC')"/>
	<action code="ModeNotSupported('ContrastMode')"/>

	<cmd code="Reset"/>		<!-- simulate unplug transducer or reopen the app -->
	<action code="PauseScan()"/>
	<!-- action code="InitializeTransducerSim('MS-400', 'Cardiology')"/ -->	<!-- with simulator -->
	<action code="InitializeTransducer('MS-400', 'Cardiology')"/>	<!-- on system -->
	<sleep time="10000"/>
	<action code="Contrast('NLC')"/>
	<action code="ModeNotSupported('ContrastMode')"/>

	<action code="=($logmsg, ' ')"/>
	<log>script:$logmsg</log>

	<!-- PA-Mode -->
	<action code="=($logmsg, 'PA-Mode')"/>
	<log level="warning">script:$logmsg</log>
	
	<action code="=($logmsg, ' ')"/>
	<log>script:$logmsg</log>

	<!-- TST-25-0101 -->
	<action code="=($logmsg, 'TST-25-0101:')"/>
	<log level="warning">script:$logmsg</log>

	<action code="PauseScan()"/>
	<!-- action code="InitializeTransducerSim('MS-400', 'General Imaging')"/ -->	<!-- with simulator -->
	<action code="InitializeTransducer('MS-400', 'General Imaging')"/>	<!-- on system -->
	<sleep time="10000"/>
	<action code="ModeNotSupported('PaMode')"/>

	<action code="PauseScan()"/>
	<!-- action code="InitializeTransducerSim('MS-400', 'Cardiology')"/ -->	<!-- with simulator -->
	<action code="InitializeTransducer('MS-400', 'Cardiology')"/>	<!-- on system -->
	<sleep time="10000"/>
	<action code="ModeNotSupported('PaMode')"/>

	<action code="=($logmsg, ' ')"/>
	<log>script:$logmsg</log>

</script>















